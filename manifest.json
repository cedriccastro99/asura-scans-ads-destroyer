{
  "manifest_version": 3,
  "name": "Asura Scans Ads Destroyer",
  "description": "Enable Asura Scans Ads to prevent annoying ads pop-up.",
  "version": "1.0",
  "icons": {
    "16": "images/asura-scans-ads-destroyer-16.png",
    "32": "images/asura-scans-ads-destoryer-32.png",
    "48": "images/asura-scans-ads-destoryer-48.png",
    "128": "images/asura-scans-ads-destoryer-128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_icon": {
      "16": "images/asura-scans-ads-destroyer-16.png",
      "32": "images/asura-scans-ads-destoryer-32.png",
      "48": "images/asura-scans-ads-destoryer-48.png",
      "128": "images/asura-scans-ads-destoryer-128.png"
    }
  },
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+X",
        "mac": "Command+Shift+X"
      }
    }
  }
}